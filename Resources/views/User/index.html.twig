{% extends 'OroUIBundle:actions:index.html.twig' %}
{% import 'OroUIBundle::macros.html.twig' as UI %}
{% set gridName = 'users-grid' %}
{% set pageTitle = 'oro.user.entity_plural_label'|trans %}

{% block navButtons %}
    {% include 'OroImportExportBundle:ImportExport:buttons.html.twig' with {
    exportProcessor: 'oro_user',
    exportTemplateProcessor: 'oro_user',
    importProcessor: 'oro_user.add_or_replace',
    importTitle: 'oro.importexport.import.entity'|trans({'%entityName%': 'oro.user.entity_plural_label'|trans}),
    dataGridName: gridName
    } %}

    {% if resource_granted('oro_user_user_create') %}
        <div class="btn-group">
            {{ UI.addButton({
            'path': path('oro_user_create'),
            'entity_label': 'oro.user.entity_label'|trans
            }) }}
        </div>
    {% endif %}
{% endblock %}
